CLANG_CXX_LANGUAGE_STANDARD = c++0x
CLANG_CXX_LIBRARY = libc++
CONFIGURATION_BUILD_DIR = $PODS_CONFIGURATION_BUILD_DIR/Cedar
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Private" "${PODS_ROOT}/Headers/Private/Cedar" "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/Blindside" "${PODS_ROOT}/Headers/Public/Cedar"
OTHER_CFLAGS = -DDEVELOPER_BIN_DIR=@\"${DEVELOPER_BIN_DIR}\"
OTHER_LDFLAGS = -l"c++"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}
PRODUCT_BUNDLE_IDENTIFIER = org.cocoapods.${PRODUCT_NAME:rfc1034identifier}
SKIP_INSTALL = YES
